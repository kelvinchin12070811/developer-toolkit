{
  "name": "developer-toolkit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run developer-toolkit:serve-ssr",
    "serve:ssr": "node dist/developer-toolkit/server/main.js",
    "build:ssr": "ng build && ng run developer-toolkit:server",
    "prerender": "ng run developer-toolkit:prerender",
    "cloudflare:build": "ng build && cp ./dist/developer-toolkit/browser/index.html ./dist/developer-toolkit/browser/404.html",
    "cloudflare:tunnel": "cloudflared tunnel --url http://localhost:4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.0.7",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.0.7",
    "@angular/platform-server": "^17.0.7",
    "@angular/pwa": "^17.0.7",
    "@angular/router": "^17.0.7",
    "@angular/service-worker": "^17.0.7",
    "@angular/ssr": "^17.0.7",
    "@types/uuid": "^9.0.1",
    "animejs": "^3.2.1",
    "bwip-js": "^3.4.0",
    "crypto-js": "^4.2.0",
    "daisyui": "^4.4.20",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "lorem-ipsum": "^2.0.8",
    "ngx-clipboard": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "uuidv7": "^0.6.3",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.7",
    "@angular/language-server": "^17.3.2",
    "@types/animejs": "^3.1.7",
    "@types/crypto-js": "^4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.2.5",
    "autoprefixer": "^10.4.14",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.23",
